@page
@model PROYECTOBD1.Pages.Variedades.PrecioVariedad.IndexModel
@{
}
@if (Model.error.Length > 0)
{
    <div class='alert alert-warning alert-dismissible fade show' role='alert'>
        <strong>@Model.error</strong>
        <button type='button' class='btn-close' data-bs-dismiss='alert' aria-label='close'></button>
    </div>
}
<table id="myTable" class="table table-bordered table-hover table-sm">
    <thead>
        <tr>

            <td>PAIS</td>
            <td>TIPO DE VARIEDAD</td>
            <td>VALIDO DESDE</td>
            <td>HASTA</td>
            <td>PRECIO</td>
            <td>CALIBRE</td>
            <td></td>
            <td></td>



        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.listaprecio)
        {
            <tr>

                <td>@item.PAIS</td>
                <td>@item.VARIEDAD</td>
                <td>@item.FECHAINICIO</td>
                <td>@item.FECHAFIN</td>
                <td>$@item.PRECIO</td>
                <td>@item.CALIBRE</td>
                <td></td>
                <td></td>


            </tr>
        }
    </tbody>
</table>
