@page
@model PROYECTOBD1.Pages.Contratos.DetalleContratos.CondicionPago.IndexModel
@{
}
@if (Model.error.Length > 0)
{
    <div class='alert alert-warning alert-dismissible fade show' role='alert'>
        <strong>@Model.error</strong>
        <button type='button' class='btn-close' data-bs-dismiss='alert' aria-label='close'></button>
    </div>
}
<table id="myTable" class="table table-bordered table-hover table-sm">
    <thead>
        <tr>

            <td>NOMBRE CLIENTE</td>
            <td>NOMBRE PRODUCTOR</td>
            <td>TIPO</td>
            <td>CUOTAS</td>
            <td>PORCENTAJE DE CUOTA</td>
            <td>CONTADO A LA EMISION</td>
            <td>CONTADO AL RECIBIR</td>
            <td></td>
            <td></td>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.listacp)
        {
            <tr>

                <td>@item.NOMBRECLIENTE</td>
                <td>@item.NOMBREPRODUCTOR</td>
                <td>@item.TIPO</td>
                <td>@item.CANTIDADCUOTA</td>
                <td>@item.PORCENTAJE_POR_CUOTA</td>
                <td>$@item.CONTADO_A_LA_EMISION</td>
                <td>$@item.CONTADO_AL_RECIBIR</td>
                <td></td>
                <td></td>



            </tr>
        }
    </tbody>
</table>
