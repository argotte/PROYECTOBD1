@page
@model PROYECTOBD1.Pages.Contratos.Renovaciones.IndexModel
@{
}
@if (Model.error.Length > 0)
{
    <div class='alert alert-warning alert-dismissible fade show' role='alert'>
        <strong>@Model.error</strong>
        <button type='button' class='btn-close' data-bs-dismiss='alert' aria-label='close'></button>
    </div>
}
<table id="myTable" class="table table-bordered table-hover table-sm">
    <thead>
        <tr>

            <td>NOMBRE CLIENTE</td>
            <td>NOMBRE PRODUCTOR</td>
            <td>FECHA DE RENOVACION</td>
            <td>TOTAL</td>

        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.listaRenovacion)
        {
            <tr>

                <td>@item.NOMBRECLIENTE</td>
                <td>@item.NOMBREPRODUCTOR</td>
                <td>@item.FECHARENOVACION</td>
                <td>@item.TOTALDOLARES$</td>

            </tr>
        }
    </tbody>
</table>

